    //     "typings": false,
    //     "tmp": true,
    //     "coverage-unit": true,
    //     "logs": true,
    //     "**/*.js.map": true,
    //     "**/*.css": {
    //         "when": "$(basename).less"
    //     },
    //     "**/*.js": {
    //         "when": "$(basename).ts"
    //     }
    // },
    "files.watcherExclude": {
        "**/.git/objects/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/dist": true,
        "**/*.js": true,
        "**/coverage-unit": true
    }
}