// Place your settings in this file to overwrite default and user settings.
{
    "explorer.openEditors.visible": 0,
    "relativePath.ignore": [
        "**/node_modules/**",
        "**/e2e/**",
        "**/dist/**",
        "**/tmp/**",
        "**/*.dll"
    ],
    "tslint.exclude": [
        "/dist/**/*.js"
    ],
    "eslint.enable": false,
    "jshint.enable": false,
    // "files.exclude": {
    //     "**/.git": true,
    //     "**/.DS_Store": true,
    //     ".idea": true,
